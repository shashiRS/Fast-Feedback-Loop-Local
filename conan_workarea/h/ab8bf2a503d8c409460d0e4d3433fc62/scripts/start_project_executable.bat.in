@echo off

REM Script for starting an application which is using the Next libraries.
REM
REM Do not change this file, it is generated during a CMake run.
REM
REM The script is starting the @STARTUPAPPLICATION@ out of the current directory. The conan cache directory is added
REM to the PATH variable. So the libraries do not have to be installed.

set PATH=@NEXTCONANCACHEDIR@\bin;%PATH%

@STARTUPAPPLICATION@.exe

pause
